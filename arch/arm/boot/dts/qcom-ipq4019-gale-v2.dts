#include "qcom-ipq4019.dtsi"
#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "Google IPQ4019/Gale";
	compatible = "google,gale-v2", "qcom,ipq4019";

	memory {
		device_type = "memory";
		reg = <0x80000000 0x20000000>; /* 512MB */
	};

	clocks {
		xo: xo {
			compatible = "fixed-clock";
			clock-frequency = <48000000>;
			#clock-cells = <0>;
		};
	};

	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
		rsvd@87000000 {
			/* Reserved for other subsystem */
			reg = <0x87000000 0x01000000>;
			no-map;
		};
	};

	firmware {
		chromeos {
			pinctrl-0 = <&fw_pins>;
			pinctrl-names = "default";
			write-protect-gpio = <&tlmm 53 GPIO_ACTIVE_LOW>;
			recovery-switch = <&tlmm 57 GPIO_ACTIVE_LOW>;
			developer-switch = <&tlmm 41 GPIO_ACTIVE_LOW>;
		};
	};

	soc {

		pinctrl@0x01000000 {

			fw_pins: fw_pinmux {
				wp {
					pins = "gpio53";
					output-low;
				};
				recovery {
					pins = "gpio57";
					bias-none;
				};
				developer {
					pins = "gpio41";
					bias-none;
				};
			};

			reset802_15_4 {
					pins = "gpio60";
			};

			led_reset {
					pins = "gpio22";
					output-high;
			};

			sys_reset {
					pins = "gpio19";
					output-high;
			};

			rx_active {
					pins = "gpio43";
					bias-pull,down;
			};

			spi_0_pins: spi_0_pinmux {
				pinmux {
					function = "blsp_spi0";
					pins = "gpio13", "gpio14","gpio15";
				};
				pinmux_cs {
					function = "gpio";
					pins = "gpio12";
				};
				pinconf {
					pins = "gpio13", "gpio14","gpio15";
					drive-strength = <12>;
					bias-disable;
				};
				pinconf_cs {
					pins = "gpio12";
					drive-strength = <2>;
					bias-disable;
					output-high;
				};
			};

			spi_1_pins: spi_1_pinmux {
				pinmux {
					function = "blsp_spi1";
					pins = "gpio44", "gpio46","gpio47";
				};
				pinmux_cs {
					function = "gpio";
					pins = "gpio45";
				};
				pinconf {
					pins = "gpio44", "gpio46","gpio47";
					drive-strength = <12>;
					bias-disable;
				};
				pinconf_cs {
					pins = "gpio45";
					drive-strength = <2>;
					bias-disable;
					output-high;
				};
			};

			serial_0_pins: serial0_pinmux {
				mux {
					pins = "gpio16", "gpio17";
					function = "blsp_uart0";
					bias-disable;
				};
			};

			serial_1_pins: serial1_pinmux {
				mux {
					pins = "gpio8", "gpio9", "gpio10", "gpio11";
					function = "blsp_uart1";
					bias-disable;
				};
			};

			i2c_0_pins: i2c_0_pinmux {
				mux {
					pins = "gpio20", "gpio21";
					function = "blsp_i2c0";
					drive-open-drain;
				};
			};

			i2c_1_pins: i2c_1_pinmux {
				mux {
					pins = "gpio34", "gpio35";
					function = "blsp_i2c1";
					drive-open-drain;
				};
			};

			sd_0_pins: sd_0_pinmux {
				sd0 {
					pins = "gpio23";
					function = "sdio0";
					drive-strength = <10>;
				};
				sd1 {
					pins = "gpio24";
					function = "sdio1";
					drive-strength = <10>;
				};
				sd2 {
					pins = "gpio25";
					function = "sdio2";
					drive-strength = <10>;
				};
				sd3 {
					pins = "gpio26";
					function = "sdio3";
					drive-strength = <10>;
				};
				sdclk {
					pins = "gpio27";
					function = "sdio_clk";
					drive-strength = <4>;
				};
				sdcmd {
					pins = "gpio28";
					function = "sdio_cmd";
					drive-strength = <10>;
				};
				sd4 {
					pins = "gpio29";
					function = "sdio4";
					drive-strength = <10>;
				};
				sd5 {
					pins = "gpio30";
					function = "sdio5";
					drive-strength = <10>;
				};
				sd6 {
					pins = "gpio31";
					function = "sdio6";
					drive-strength = <10>;
				};
				sd7 {
					pins = "gpio32";
					function = "sdio7";
					drive-strength = <10>;
					bias-disable;
				};
			};

			mdio_pins: mdio_pinmux {
				mux_1 {
					pins = "gpio6";
					function = "mdio0";
					bias-disable;
				};
				mux_2 {
					pins = "gpio7";
					function = "mdc";
					bias-disable;
				};
				mux_3 {
					pins = "gpio40";
					function = "gpio";
					bias-disable;
					output-high;
				};
			};
		};

		blsp_dma: dma@7884000 {
			status = "ok";
		};

		serial@78af000 {
			pinctrl-0 = <&serial_0_pins>;
			pinctrl-names = "default";
			status = "ok";
		};

		serial@78b0000 {
			pinctrl-0 = <&serial_1_pins>;
			pinctrl-names = "default";
			status = "ok";
		};

		usb3_ss_phy: ssphy@0 {
			status = "ok";
		};

		dummy_ss_phy: ssphy@1 {
			status = "ok";
		};

		usb3_hs_phy: hsphy@a6000 {
			status = "ok";
		};

		usb2_hs_phy: hsphy@a8000 {
			status = "ok";
		};

		usb3: usb3@8a00000 {
			status = "ok";
		};

		usb2: usb2@6000000 {
			status = "ok";
		};

		spi_0: spi@78b5000 { /* BLSP1 QUP1 */
			pinctrl-0 = <&spi_0_pins>;
			pinctrl-names = "default";
			status = "ok";
			cs-gpios = <&tlmm 12 0>;

			spidev@0 {
				compatible = "spidev";
				reg = <0>;
				spi-max-frequency = <24000000>;
			};
		};

		spi_1: spi@78b6000 { /* BLSP1 QUP2 */
			pinctrl-0 = <&spi_1_pins>;
			pinctrl-names = "default";
			status = "ok";
			cs-gpios = <&tlmm 45 0>;

			spidev@0 {
				compatible = "spidev";
				reg = <0>;
				spi-max-frequency = <24000000>;
			};
		};

		i2c_0: i2c@78b7000 { /* BLSP1 QUP2 */
			pinctrl-0 = <&i2c_0_pins>;
			pinctrl-names = "default";
			status = "ok";
			trusted-platform-module {
				compatible = "infineon,slb9645tt";
				reg = <0x20>;
				powered-while-suspended;
			};
		};

		i2c_1: i2c@78b8000 { /* BLSP1 QUP3 */
			pinctrl-0 = <&i2c_1_pins>;
			pinctrl-names = "default";
			status = "ok";
			lp55231@32 {
				compatible = "national,lp5523";
				reg = <0x32>;
				clock-mode = [01];

				chan0 {
					chan-name = "LED0_Red";
					led-cur = [64];
					max-cur = [78];
				};

				chan1 {
					chan-name = "LED0_Green";
					led-cur = [64];
					max-cur = [78];
				};

				chan2 {
					chan-name = "LED0_Blue";
					led-cur = [64];
					max-cur = [78];
				};

				chan3 {
					chan-name = "LED1_Red";
					led-cur = [64];
					max-cur = [78];
				};

				chan4 {
					chan-name = "LED1_Green";
					led-cur = [64];
					max-cur = [78];
				};

				chan5 {
					chan-name = "LED1_Blue";
					led-cur = [64];
					max-cur = [78];
				};

				chan6 {
					chan-name = "LED2_Red";
					led-cur = [64];
					max-cur = [78];
				};

				chan7 {
					chan-name = "LED2_Green";
					led-cur = [64];
					max-cur = [78];
				};

				chan8 {
					chan-name = "LED2_Blue";
					led-cur = [64];
					max-cur = [78];
				};
			};
		};

		rng@0x00022000 {
			status = "ok";
		};

		qcom_crypto: qcrypto@8e20000 {
			status = "ok";
		};

		qcom_cedev: qcedev@8e20000 {
			status = "ok";
		};

		usb3: usb3@8a00000 {
			status = "ok";
		};

		usb2: usb2@6000000 {
			status = "ok";
		};

		vccq_sd0: regulator@0 {
			status = "ok";
		};

		sdhci@7824000 {
			status = "ok";
			pinctrl-0 = <&sd_0_pins>;
			pinctrl-names = "default";
			vqmmc-supply = <&vccq_sd0>;
			non-removable;
		};

		ess-switch@c000000 {
			switch_lan_bmp = <0x10>;
			switch_wan_bmp = <0x20>;
			switch_mac_mode = <0x0>;
		};

		mdio@90000 {
			status = "ok";
			pinctrl-0 = <&mdio_pins>;
			pinctrl-names = "default";
		};

		watchdog@b017000 {
			status = "ok";
		};

	};
};
